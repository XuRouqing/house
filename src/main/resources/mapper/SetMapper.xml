<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.house.dao.SetMapper">

    <insert id="insertSet" parameterType="Set">
        insert into myset (setId,name,pic)
        values (#{setId},#{name},#{pic})
    </insert>

    <select id="selectSetById" resultType="Set">
        select * from myset
            where setId = #{setId}
    </select>

    <select id="selectSetAll" resultType="Set">
        select (@i:=@i+1) rownum,myset.* from myset, (SELECT @i:=0) t2
    </select>

    <update id="modifySet" parameterType="Set">
        UPDATE myset SET name=#{name},gender=#{gender},workingAge=#{workingAge},level=#{level},style=#{style},represent=#{represent},tel=#{tel},age=#{age}
        WHERE setId=#{setId}
    </update>

    <delete id="deleteSet" parameterType="Set">
        DELETE FROM myset
        WHERE setId=#{setId}
    </delete>

</mapper>
